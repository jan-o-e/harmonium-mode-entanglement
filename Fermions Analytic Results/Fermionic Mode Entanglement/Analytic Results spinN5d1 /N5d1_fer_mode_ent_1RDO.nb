Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "the", " ", "intxmab", " ", "function", " ", "is", " ", "the", " ", 
     "results", " ", "of", " ", "the", " ", "following", " ", 
     RowBox[{"integral", ":", "\[IndentingNewLine]", " ", 
      RowBox[{
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"x", "^", "m"}], "*", " ", 
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"a", " ", 
             RowBox[{"x", "^", "2"}]}], "+", 
            RowBox[{"b", " ", "x"}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
         " ", 
         RowBox[{"Assumptions", "\[Rule]", 
          RowBox[{
           RowBox[{"a", "<", "0"}], " ", "&&", " ", 
           RowBox[{"m", "\[Element]", "Integers"}], " ", "&&", " ", 
           RowBox[{
            RowBox[{"Im", "[", "b", "]"}], "==", "0"}]}]}]}], "]"}], 
       "*"}]}]}], ",", " ", 
    RowBox[{
    "it", " ", "gives", " ", "the", " ", "integral", " ", "over", " ", "all", 
     " ", "space", " ", "of", " ", 
     RowBox[{"x", "^", "m"}], "*", " ", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"a", " ", 
        RowBox[{"x", "^", "2"}]}], "+", 
       RowBox[{"b", " ", "x"}]}], "]"}]}], ",", " ", 
    RowBox[{
    "where", " ", "the", " ", "function", " ", "inputs", " ", "correspond", 
     " ", "to", " ", "the", " ", "polynomial", " ", "power"}], ",", " ", 
    RowBox[{
    "quadratic", " ", "and", " ", "linear", " ", "exponential", " ", 
     "coefficient", " ", "respectively"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"intxmab", "[", 
     RowBox[{"m_", ",", " ", "a_", ",", " ", "b_"}], "]"}], ":=", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "a"}], ")"}], 
      RowBox[{
       RowBox[{"-", "m"}], "/", "2"}]], " ", 
