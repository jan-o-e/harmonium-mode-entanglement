Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "the", " ", "intxmab", " ", "function", " ", "is", " ", "the", " ", 
     "results", " ", "of", " ", "the", " ", "following", " ", 
     RowBox[{"integral", ":", "\[IndentingNewLine]", " ", 
      RowBox[{
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"x", "^", "m"}], "*", " ", 
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"a", " ", 
             RowBox[{"x", "^", "2"}]}], "+", 
            RowBox[{"b", " ", "x"}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
         " ", 
         RowBox[{"Assumptions", "\[Rule]", 
          RowBox[{
           RowBox[{"a", "<", "0"}], " ", "&&", " ", 
           RowBox[{"m", "\[Element]", "Integers"}], " ", "&&", " ", 
           RowBox[{
            RowBox[{"Im", "[", "b", "]"}], "==", "0"}]}]}]}], "]"}], 
       "*"}]}]}], ",", " ", 
    RowBox[{
    "it", " ", "gives", " ", "the", " ", "integral", " ", "over", " ", "all", 
     " ", "space", " ", "of", " ", 
     RowBox[{"x", "^", "m"}], "*", " ", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"a", " ", 
        RowBox[{"x", "^", "2"}]}], "+", 
       RowBox[{"b", " ", "x"}]}], "]"}]}], ",", " ", 
    RowBox[{
    "where", " ", "the", " ", "function", " ", "inputs", " ", "correspond", 
     " ", "to", " ", "the", " ", "polynomial", " ", "power"}], ",", " ", 
    RowBox[{
    "quadratic", " ", "and", " ", "linear", " ", "exponential", " ", 
     "coefficient", " ", "respectively"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"intxmab", "[", 
     RowBox[{"m_", ",", " ", "a_", ",", " ", "b_"}], "]"}], ":=", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "a"}], ")"}], 
      RowBox[{
       RowBox[{"-", "m"}], "/", "2"}]], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox["1", "a"], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "1"}], "+", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"-", "1"}], ")"}], "m"]}], ")"}], " ", "b", " ", 
         RowBox[{"Gamma", "[", 
          RowBox[{"1", "+", 
           FractionBox["m", "2"]}], "]"}], " ", 
         RowBox[{"Hypergeometric1F1", "[", 
          RowBox[{
           RowBox[{"1", "+", 
            FractionBox["m", "2"]}], ",", 
           FractionBox["3", "2"], ",", 
           RowBox[{"-", 
            FractionBox[
             SuperscriptBox["b", "2"], 
             RowBox[{"4", " ", "a"}]]}]}], "]"}]}]}], "+", 
       FractionBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "+", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"-", "1"}], ")"}], "m"]}], ")"}], " ", 
         RowBox[{"Gamma", "[", 
          FractionBox[
           RowBox[{"1", "+", "m"}], "2"], "]"}], " ", 
         RowBox[{"Hypergeometric1F1", "[", 
          RowBox[{
           FractionBox[
            RowBox[{"1", "+", "m"}], "2"], ",", 
           FractionBox["1", "2"], ",", 
           RowBox[{"-", 
            FractionBox[
             SuperscriptBox["b", "2"], 
             RowBox[{"4", " ", "a"}]]}]}], "]"}]}], 
        SqrtBox[
         RowBox[{"-", "a"}]]]}], ")"}]}]}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "intxmabpoly", " ", "returns", " ", "the", " ", "polynomial", " ", 
     "contribution", " ", "of", " ", "the", " ", "integration", " ", 
     "inxmab"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"intxmabpoly", "[", 
     RowBox[{"m_", ",", "a_", ",", "b_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{"intxmab", "[", 
      RowBox[{"m", ",", "a", ",", "b"}], "]"}], "*", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"(", 
